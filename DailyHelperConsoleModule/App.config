<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.serviceModel>
    <client>
      <endpoint name = "SaveNoteEndpoint"
                address = "net.tcp://localhost:8888/DataSaverService"
                binding = "netTcpBinding"
                bindingConfiguration = "SimpleTCP"
                contract = "DailyHelperLibrary.ServiceContracts.INoteSaverService"
      />
      <endpoint name = "SaveUserEndpoint"
                address = "net.tcp://localhost:8888/DataSaverService"
                binding = "netTcpBinding"
                bindingConfiguration = "SimpleTCP"
                contract = "DailyHelperLibrary.ServiceContracts.IUserSaverService"
      />
      <endpoint name = "SaveTodoEndpoint"
                address = "net.tcp://localhost:8888/DataSaverService"
                binding = "netTcpBinding"
                bindingConfiguration = "SimpleTCP"
                contract = "DailyHelperLibrary.ServiceContracts.ITodoSaverService"
      />
      <endpoint name = "SaveAccountInfoEndpoint"
                address = "net.tcp://localhost:8888/DataSaverService"
                binding = "netTcpBinding"
                bindingConfiguration = "SimpleTCP"
                contract = "DailyHelperLibrary.ServiceContracts.ISocialNetworkAccountInfoSaverService"
      />
      <endpoint name = "GetMusicStreamEndpoint"
                address = "net.tcp://localhost:8008/DataSaverService"
                binding = "netTcpBinding"
                bindingConfiguration = "StreamedTCP"
                contract = "DailyHelperLibrary.ServiceContracts.IMusicStreamGetterService"
      />
      <endpoint name = "SaveScheduleItemEndpoint"
                address = "net.tcp://localhost:8888/DataSaverService"
                binding = "netTcpBinding"
                bindingConfiguration = "SimpleTCP"
                contract = "DailyHelperLibrary.ServiceContracts.IScheduleItemSaverService"
      />
    </client>
    <bindings>
      <netTcpBinding>
        <binding name = "StreamedTCP"
                 transferMode = "Streamed"
                 maxReceivedMessageSize = "640000000"
                 receiveTimeout = "24:00:00">
          <security mode = "None"/>        
        </binding>
        <binding name = "SimpleTCP">
          <security mode = "None"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>